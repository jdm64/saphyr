@x = global i32 0

define void @A_null(%A* %this) {
  %1 = alloca %A*
  store %A* %this, %A** %1
  %2 = load i32, i32* @x
  %3 = add i32 %2, 1
  store i32 %3, i32* @x
  ret void
}

define void @B_null(%B* %this) {
  %1 = alloca %B*
  store %B* %this, %B** %1
  %2 = load i32, i32* @x
  %3 = add i32 %2, -1
  store i32 %3, i32* @x
  %4 = load %B*, %B** %1
  %5 = getelementptr %B, %B* %4, i32 0, i32 0
  call void @A_null(%A* %5)
  ret void
}

define void @C_null(%C* %this) {
  %1 = alloca %C*
  store %C* %this, %C** %1
  %2 = load %C*, %C** %1
  %3 = getelementptr %C, %C* %2, i32 0, i32 0
  call void @A_null(%A* %3)
  ret void
}

define void @D_null(%D* %this) {
  %1 = alloca %D*
  store %D* %this, %D** %1
  %2 = load %D*, %D** %1
  %3 = getelementptr %D, %D* %2, i32 0, i32 0
  %4 = load [0 x i32]*, [0 x i32]** %3
  %5 = bitcast [0 x i32]* %4 to i8*
  call void @free(i8* %5)
  ret void
}

declare void @free(i8*)

define void @D_clear(%D* %this) {
  %1 = alloca %D*
  store %D* %this, %D** %1
  %2 = load %D*, %D** %1
  call void @D_null(%D* %2)
  ret void
}

