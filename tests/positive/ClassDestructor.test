
int x = 0;

class A
{
	~this()
	{
		x++;
	}
}

class B
{
	struct this
	{
		A a;
	}

	~this()
	{
		x--;
	}
}

class C
{
	struct this
	{
		A a;
	}
}

class D
{
	struct this
	{
		@[]int p;
	}

	~this()
	{
		delete p;
	}

	void clear()
	{
		~this();
	}
}

