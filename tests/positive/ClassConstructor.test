
class Empty
{
	this(int a)
	{
		a = 7;
	}
}

class MyClass
{
	struct this
	{
		int a, b;
	}

	this()
	{
		a = 4;
		b = 2;
	}

	@this makeNew()
	{
		return new this;
	}

	~this()
	{
		a = 3;
	}
}

class Both
{
	struct this
	{
		[2]MyClass a;
		[4]Empty b;
	}

	this()
	b{6}
	{
	}
}

int main()
{
	Empty a{1};
	MyClass b{};

	return 0;
}

void foo()
{
	@MyClass m = new MyClass;

	delete m;
}

void arrayLocal()
{
	[5]Empty a{2};
	[3]MyClass b;
}

void arrayNew()
{
	auto x = new [7]Empty{8};
	auto z = new [9]MyClass;
}

void testBoth()
{
	Both b;
}

class String
{
	struct this
	{
		@[]int8 ptr;
	}

	this()
	ptr{null}
	{
	}

	#[mangle("this2")]
	this(String str)
	ptr{str.ptr}
	{
	}
}

String retString()
{
	String s;
	return s;
}

void initStr()
{
	String s = retString();
}

@[]MyClass classArrayWithConstructor(int a)
{
	return new[a + 5]MyClass;
}

